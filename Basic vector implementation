// Online C++ compiler to run C++ program online
#include <iostream>
// #include <vector>            // only run vector data structure of STL...
#include <bits/stdc++.h>        // this library can run any STL data structure...
using namespace std;

class solution {
    public:
        void moveZeros(vector<int> vec) {
            cout << "capacity of vector: " << vec.capacity() << endl;
        }
        
        void printVec(vector<int> vec) {
            int size = vec.capacity();
            cout << "vector elements are:" << endl;
            for(int i:vec) {        // bs yaha dhyan rkhna hai (int i:vec)
                cout << i << " ";   // or "i" ko print krdo
            }
            cout << endl;
        }
};

int main() {
    
    solution obj;
    vector<int> vec;
    
    vec.push_back(0);
    vec.push_back(1);
    vec.push_back(0);
    vec.push_back(3);
    vec.push_back(12);
    
    obj.printVec(vec);
    obj.moveZeros(vec);
    return 0;
}
